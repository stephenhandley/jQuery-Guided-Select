use like this:

html:
<div id="leagues"></div>

js:
$('#leagues').guidedSelect({
  name: 'url_type',
  options: [
    ['Football', 'NFL'], 
    ['Basketball', 'NBA'], 
    ['Baseball', 'MLB']
  ],
  prompt: 'SPORTS!',
  userInputPrompt: '<some other sport>' 
});

